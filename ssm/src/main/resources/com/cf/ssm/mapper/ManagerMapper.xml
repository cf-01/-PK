<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cf.ssm.mapper.ManagerMapper">


    <select id="login" resultType="com.cf.ssm.pojo.Manager">
        select * from tb_manager where manager_account = #{managerAccount} and manager_password =#{managerPassword}
    </select>

</mapper>